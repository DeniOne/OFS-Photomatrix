# Настройки бота
BOT_TOKEN=your_bot_token_here
ADMIN_IDS=123456789,987654321

# Настройки API
API_URL=http://localhost:8000/api/v1
API_KEY=your_api_key_here
API_WEBHOOK_ENDPOINT=http://localhost:8000/api/v1/telegram-bot/webhook
API_TOKEN_VALIDATION_ENDPOINT=http://localhost:8000/api/v1/telegram-bot/validate-token

# Настройки хранилища
STORAGE_PATH=./data

# Настройки логирования
LOG_LEVEL=INFO
LOG_FILE=logs/bot.log

# Настройки безопасности
MAX_REQUESTS_PER_MINUTE=30
MAX_REGISTRATION_ATTEMPTS=3

# Настройки Redis (необязательно)
USE_REDIS=False
REDIS_URL=redis://localhost:6379/0

# Настройки базы данных для прямой синхронизации
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=photomatrix 